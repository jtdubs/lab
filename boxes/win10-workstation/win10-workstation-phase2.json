{
    "builders": [
        {
            "type": "vmware-vmx",
            "source_path": "output-vmware-iso-phase1/win10-base.vmx",
            "shutdown_command": "powershell Stop-Computer",
            "communicator": "winrm",
            "winrm_username": "jtdubs",
            "winrm_password": "",
            "winrm_timeout": "10m",
            "boot_wait": "10s",
            "vm_name": "win10-workstation",
            "display_name": "win10-workstation"
        },
        {
            "type": "virtualbox-ovf",
            "source_path": "output-virtualbox-iso-phase1/win10-base.ovf",
            "shutdown_command": "powershell Stop-Computer",
            "communicator": "winrm",
            "winrm_username": "jtdubs",
            "winrm_password": "",
            "winrm_timeout": "10m",
            "boot_wait": "10s",
            "vm_name": "win10-workstation"
        },
        {
            "type": "hyperv-vmcx",
            "clone_from_vmcx_path": "output-hyperv-iso-phase1",
            "shutdown_command": "powershell Stop-Computer",
            "communicator": "winrm",
            "winrm_username": "jtdubs",
            "winrm_password": "",
            "winrm_timeout": "10m",
            "boot_wait": "5s",
            "cpus": 4,
            "generation": 2,
            "headless": false,
            "memory": 4096,
            "switch_name": "Default Switch",
            "vm_name": "win10-workstation"
        }
    ],

    "provisioners": [
        {
            "type": "powershell",
            "scripts": [
                "scripts/Install-JTDubs.ps1"
            ]
        }
    ],

    "post-processors": [
        {
            "type": "vagrant",
            "keep_input_artifact": true,
            "output": "win10-workstation-{{ .Provider }}.box"
        }
    ]
}
